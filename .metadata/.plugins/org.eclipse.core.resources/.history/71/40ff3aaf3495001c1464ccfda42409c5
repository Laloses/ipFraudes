package com.ibm.academia.restapi.ipfraude.modelo.mapper;

import java.util.HashMap;

import com.ibm.academia.restapi.ipfraude.modelo.dto.CountryDTO;

public class CountryLayerMapper {
	
	public static CountryDTO mapCountryFromLayer(HashMap<String, Object> countryObject) {
		CountryDTO countryDTO = new CountryDTO();
		countryDTO.setNombre(countryObject.get("nombre"));
		countryDTO.setMoneda(countryObject.get("nombre"));
		countryDTO.setCotizacion(countryObject.get("nombre"));
		return countryDTO;
	}
}
