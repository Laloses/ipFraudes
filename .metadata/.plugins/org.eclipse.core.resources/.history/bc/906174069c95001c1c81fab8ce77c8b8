package com.ibm.academia.restapi.ipfraude.modelo.mapper;

import java.util.HashMap;

import com.ibm.academia.restapi.ipfraude.modelo.dto.Ip2CountryDTO;

public class Ip2CountryMapper {
	public static Ip2CountryDTO mapCountryFromIP(HashMap<String, String> countryObject) {
		Ip2CountryDTO ip2CountryDTO = new Ip2CountryDTO();
		ip2CountryDTO.setISO(countryObject.get("ISO"));
		ip2CountryDTO.setNombre(countryObject.getNombre());
		return ip2CountryDTO;
	}
}
