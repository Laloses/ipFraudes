package com.ibm.academia.restapi.ipfraude.modelo.mapper;

import java.util.HashMap;

import com.ibm.academia.restapi.ipfraude.modelo.dto.CountryDTO;

public class CountryMapper {
	static CountryDTO mapCountryFromIP(HashMap<String, String> countryObject) {
		CountryDTO countryDTO = new CountryDTO();
		countryDTO.setISO(countryObject.get("ISO"));
		return countryDTO;
	}
	
	static CountryDTO mapCountryFromLayer(HashMap<String, String> countryObject) {
		CountryDTO countryDTO = new CountryDTO();
		countryDTO.setNombre(countryObject.get("ISO"));
		return countryDTO;
	}
}
