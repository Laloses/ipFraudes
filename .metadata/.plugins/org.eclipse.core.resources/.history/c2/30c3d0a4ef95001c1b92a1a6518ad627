package com.ibm.academia.restapi.ipfraude.modelo.dto;

import java.io.Serializable;
import java.util.Map;

import com.fasterxml.jackson.annotation.JsonProperty;

import lombok.Getter;
import lombok.Setter;

@Setter
@Getter
public class CountryLayerDTO implements Serializable {
	private String moneda;
	private String cotizacion;
	
	@JsonProperty("currencies")
	private void unpackNameFromNestedObject(Map<String, Object> brand) {
		moneda = brand.get("name");
		cotizacion = brand.get("code");
	}
	/**
	 * 
	 */
	private static final long serialVersionUID = 2366095346076959598L;
	
}